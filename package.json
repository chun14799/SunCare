{
  "name": "react-tailwindui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.18.7",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-slick": "^0.28.1",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "set PORT=3011 && react-scripts start",
    "start-macos": "PORT=3011 react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "after-build": "rm -rf after_build; mv build after_build",
    "build:local": "env-cmd -f .env.local npm run-script build; npm run-script after-build",
    "build:dev": "env-cmd -f .env.dev npm run-script build; npm run-script after-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css": "^3.0.0",
    "css-loader": "^6.6.0",
    "react-api-codeby": "^1.0.5",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.7",
    "webpack": "^5.69.1"
  }
}
